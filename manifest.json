{
  "manifest_version": 2,
  "name": "Definitorium",
  "version": "0.5",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "assets/definitorium_icon.png",
    "default_title": "Definitions will be here",
    "default_popup": "popup.html"
  }
}